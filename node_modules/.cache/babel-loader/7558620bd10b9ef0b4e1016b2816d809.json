A;;AAEA,OAAO,SAASU,KAAT,CAAehB,SAAf,EAAyBC,QAAzB,EAAkCK,IAAlC,EAAwC;AAC7C,SAAOR,OAAO,CAAE;AACZI,IAAAA,GAAG,EAAE,qBADO;AAEZC,IAAAA,MAAM,EAAE,KAFI;AAGZC,IAAAA,MAAM,EAAE;AAACJ,MAAAA,SAAS,EAATA,SAAD;AAAWC,MAAAA,QAAQ,EAARA,QAAX;AAAoBK,MAAAA,IAAI,EAAJA;AAApB;AAHI,GAAF,CAAd;AAKD","sourcesContent":["import request from '@/utils/request'\r\n\r\n/**\r\n * 账号管理 - 获取所有小程序用户\r\n */\r\n\r\nexport function getAccountList(pageIndex,pageSize) {\r\n    return request ({\r\n        url: '/admin/users',\r\n        method: 'get',\r\n        params: {pageIndex,pageSize}\r\n    })\r\n}\r\n\r\n/**\r\n *账号管理 - 根据姓名查找用户\r\n */\r\n\r\nexport function searchAccount(pageIndex,pageSize,name) {\r\n    return request ({\r\n        url: '/admin/usersOfName',\r\n        method: 'get',\r\n        params: {pageIndex,pageSize,name}\r\n    })\r\n}\r\n\r\n/**\r\n *账号管理 - 根据用户查找预约\r\n */\r\n\r\nexport function searchAppoint(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/reservationsOfUser',\r\n      method: 'get',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 查看预约详情\r\n */\r\n\r\nexport function getAppointDetail(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/reservationDetail',\r\n      method: 'get',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 下拉获取管理员老师列表\r\n */\r\n\r\nexport function getAdminsList(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/assignableAdmins',\r\n      method: 'get',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n\r\n/**\r\n *账号管理 - 指派\r\n */\r\n\r\nexport function Appoint(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/assignReservation',\r\n      method: 'put',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 认领\r\n */\r\n\r\nexport function Claim(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/claimReservation',\r\n      method: 'put',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 确认\r\n */\r\n\r\nexport function Confirm(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/confirmReservationSolvedDatetime',\r\n      method: 'put',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 完成预约\r\n */\r\n\r\n export function FinishReserve(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/finishReservation',\r\n      method: 'put',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 根据用户查找留言\r\n */\r\n\r\nexport function searchMessage(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/messagesOfUser',\r\n      method: 'get',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 留言详情\r\n */\r\n\r\nexport function getServiceDetail(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/messageDetail',\r\n      method: 'get',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 回复留言\r\n */\r\n\r\nexport function Reply(pageIndex,pageSize,name) {\r\n  return request ({\r\n      url: '/admin/replyMessage',\r\n      method: 'put',\r\n      params: {pageIndex,pageSize,name}\r\n  })\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"]}]}