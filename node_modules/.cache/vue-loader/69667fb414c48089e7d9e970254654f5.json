umn prop=\"bookerGrade\" label=\"年级\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerGenderString\" label=\"性别\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solutionString\" label=\"形式\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"statusString\" label=\"状态\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solverName\" label=\"对接老师\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solvedDatetimeString\" label=\"时间\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n          <template slot-scope=\"scope\">\r\n            <el-row :gutter=\"5\">\r\n              <el-col :span=\"10\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleGetAppointDetail(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >查看详情</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.statusString=='待指派'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleAppoint(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >指派</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.statusString=='待指派'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleClaim(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >认领</el-button>\r\n              </el-col>\r\n              <el-col :span=\"14\" v-show=\"scope.row.statusString=='待确认'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleAllocateTime(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >分配时间</el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogAppointDetailVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"appointDetail\" style=\"width: 100%\">\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"situationDesc\" label=\"主题描述\" width=\"360\"></el-table-column>\r\n        <el-table-column prop=\"reservedDatetimes\" label=\"有空时段\" width=\"120\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog title=\"指派\" :visible.sync=\"dialogAppointVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-select v-model=\"adminsList.name\" clearable placeholder=\"请选择老师\" style=\"width:70%\">\r\n        <el-option\r\n          v-for=\"item in adminsList\"\r\n          :key=\"item.aid\"\r\n          :label=\"item.name\"\r\n          :value=\"item.name\">\r\n        </el-option>\r\n      </el-select>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"handleAppoint(scope.row)\">确 定</el-button>\r\n        <el-button @click=\"dialogAppointVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"认领\" :visible.sync=\"dialogClaimVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <span>认领该生？</span>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogClaimVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogClaimVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"分配时间\" :visible.sync=\"dialogAllocateTimeVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <div style=\"margin-bottom: 10px; display: flex; flex-direction: column\">\r\n        <span style=\"padding: 10px 0px; font-size: 15px\">时间描述</span>\r\n        <span style=\"padding: 20px 0px; color: #A9A9A90\">{{ solvedDatetimeString }}</span>\r\n        <span style=\"padding: 10px 0px 40px; font-size: 15px\">分配时间</span>\r\n        <el-row>\r\n          <el-col :span=\"8\">\r\n            <el-date-picker\r\n              v-model=\"date\"\r\n              type=\"date\"\r\n              placeholder=\"请选择日期\"\r\n              :picker-options=\"pickerOptions\">\r\n            </el-date-picker>\r\n          </el-col>  \r\n          <el-col :span=\"16\">\r\n            <el-time-picker\r\n              v-model=\"time\"\r\n              :picker-options=\"{\r\n                selectableRange: '18:30:00 - 20:30:00'\r\n              }\"\r\n              placeholder=\"请选择开始时间\">\r\n            </el-time-picker>\r\n          </el-col>  \r\n          <el-col :span=\"12\" :offset=\"20\">\r\n            <el-button @click=\"dialogAllocateTimeVisible = false\">确 定</el-button>\r\n          </el-col>  \r\n        </el-row>  \r\n      </div>\r\n    </el-dialog>\r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAppointDetail, getAdminsList, Appoint, Claim, Confirm, FinishReserve } from \"@/api/account\";\r\nimport { getAppointList } from \"@/api/appoint\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      appointList: [],\r\n      adminsList: [],\r\n      checkedList: [],\r\n      appointDetail: {\r\n        \r\n      },\r\n      dialogAppointVisible: false,\r\n      dialogClaimVisible: false,\r\n      dialogAllocateTimeVisible: false,\r\n      dialogAppointDetailVisible: false,\r\n      totalCount:0,\r\n      pageIndex:0\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchAppointList(pageIndex) {\r\n      getAppointList(pageIndex)\r\n        .then(res => {\r\n          res.data.pageList.map(item=>{\r\n          delete item.bookerId\r\n          delete item.bookerRole\r\n          delete item.bookerGender\r\n          delete item.solution\r\n          delete item.status\r\n          delete item.solvedDatetime\r\n          })\r\n          //console.log(res);\r\n          this.appointList = res.data.pageList;\r\n          this.totalCount = res.data.totalRecord;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    //获取预约详情\r\n    handleGetAppointDetail(row) {\r\n      this.dialogAppointDetailVisible = true;//传入rid\r\n      getAppointDetail(row.rid)\r\n        .then(res => {\r\n        this.dialogAppointDetailVisible = true;\r\n        this.appointDetail = res.data;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    //指派\r\n    handleAppoint(row){\r\n      getAdminsList(pageIndex)\r\n        .then(res => {\r\n          this.adminsList=res.data.data.pageList;\r\n          this.dialogAppointVisible = true;\r\n        })\r\n      const params = new FormData();\r\n      // params.append(\"aid\",row.aid);\r\n      Appoint(row.rid, assignedAid)\r\n        .then(res => {\r\n          this.adminsList=res.data.data.pageList;\r\n          this.dialogAppointVisible = true;\r\n        })\r\n    },\r\n\r\n    //认领\r\n    handleClaim(row){\r\n      this.dialogClaimVisible = true;\r\n    },\r\n\r\n    //分配时间\r\n    handleAllocateTime(row){\r\n      this.dialogAllocateTimeVisible = true;\r\n    },\r\n\r\n\r\n    // 分页\r\n    handlePageChange(page){\r\n      this.pageIndex = page-1;\r\n      console.log(this.pageIndex)\r\n      this.fetchpersonalList(this.pageIndex);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchAppointList(0);\r\n  }\r\n};\r\n</script>"]}]}