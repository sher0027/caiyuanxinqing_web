n            @click=\"handleSearch\"\r\n          >搜索</el-button>\r\n        </div>\r\n      </el-col>\r\n    </el-row>\r\n    <el-row style=\"padding:10px\">\r\n      <el-table :data=\"clientList\" style=\"width: 100%\">\r\n      <el-table-column type=\"index\" width=\"0\"></el-table-column>\r\n      <el-table-column prop=\"ID\" label=\"ID\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"avatar\" label=\"头像\" width=\"80\">\r\n          <template slot-scope=\"scope\">\r\n            <el-avatar shape=\"square\" size=\"large\" :src=\"scope.row.avatar\"></el-avatar>\r\n          </template>\r\n        </el-table-column>\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"120\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"手机号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"regisTime\" label=\"注册时间\" width=\"90\"></el-table-column>\r\n      <el-table-column prop=\"lastloginTime\" label=\"最后登录时间\" width=\"90\"></el-table-column>\r\n      <el-table-column prop=\"bindinfo\" label=\"绑定信息\" width=\"90\"></el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"email\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from \"../../utils\";\r\nimport { getAccountList, getAccount } from \"@/api/account\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogTableVisible: false,\r\n      clientList: [],\r\n      detailList: [],\r\n      totalCount:0,\r\n      pageIndex:0\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchAccountList(pageIndex) {\r\n      getAccountList(pageIndex)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.clientList = res.data;\r\n          this.totalCount = res.data[0].count;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    handleGetDetail(row) {\r\n      const fid = row.fid;\r\n      getAccountInsure(fid)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.detailList = res.data;\r\n          this.dialogTableVisible = true;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    filterFlag(value, row) {\r\n      return row.flag === value;\r\n    },\r\n    isDetailDisabled(index) {\r\n      if (this.clientList[index].flag) {\r\n        return false;\r\n      } else {\r\n        return true;\r\n      }\r\n    },\r\n\r\n    // 规范时间\r\n    formatTime(time) {\r\n      return parseTime(time);\r\n    },\r\n\r\n        // 分页\r\n    handlePageChange(page){\r\n      this.pageIndex = page-1;\r\n      console.log(this.pageIndex)\r\n      this.fetchAccountList(this.pageIndex);\r\n    },\r\n  // 删除\r\n    handleDelete(row) {\r\n      console.log(row);\r\n      deleteGuarantee(row.gid)\r\n        .then(res => {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"已删除！\"\r\n          });\r\n          this.fetchGuaranteeList(this.pageIndex);\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    filterFlag(value, row) {\r\n      return row.state === value;\r\n    },\r\n    handleGetFlag(flag) {\r\n      for (let i = 0; i < 5; i++) {\r\n        if (flag == this.statusFilter[i].value) {\r\n          return this.statusFilter[i].text;\r\n        }\r\n      }\r\n    },\r\n\r\n    // 改变状态\r\n    handleChangeState(row){\r\n      const params = new FormData();\r\n      params.append(\"gid\",row.gid);\r\n      params.append(\"state\", this.currentState);\r\n      changeState(params).then(res =>{\r\n        this.$message({\r\n          type:'success',\r\n          message:\"修改状态成功！\"\r\n        });\r\n        this.fetchGuaranteeList(this.pageIndex);\r\n      }).catch(err =>{\r\n        this.$message({\r\n          type:\"error\",\r\n          message:\"修改失败，请稍后重试！\"\r\n        })\r\n        console.log(err)\r\n      })\r\n    },\r\n\r\n    handlePopover(row){\r\n      this.currentState = row.state;\r\n      //console.log(this.currentState)\r\n    },\r\n\r\n    // 搜索保单\r\n    handleSearch(){\r\n      console.log(this.searchKey);\r\n      const key = this.searchKey;\r\n      searchGuarantee(key, 0).then(res=>{\r\n        this.orderList = res.data;\r\n        this.totalCount = res.data.length\r\n      }).catch( err => {\r\n        console.log(err)\r\n      })\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchAccountList(0);\r\n  }\r\n};\r\n</script>\r\n"]}]}