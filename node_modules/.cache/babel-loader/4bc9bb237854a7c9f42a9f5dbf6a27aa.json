model=\"orderForm.type\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"投保人\" prop=\"insure\">\r\n            <el-input v-model=\"orderForm.insure\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"被保人\" prop=\"name\">\r\n            <el-input v-model=\"orderForm.name\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"身份证号\" prop=\"identify\">\r\n            <el-input v-model=\"orderForm.identify\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"序列号PIN\" prop=\"pin\">\r\n            <el-input v-model=\"orderForm.pin\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"起保日期\" prop=\"startTime\">\r\n            <el-date-picker\r\n              v-model=\"orderForm.startTime\"\r\n              type=\"date\"\r\n              placeholder=\"选择起保日期\"\r\n              value-format=\"yyyy-MM-dd\"\r\n            ></el-date-picker>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"截止日期\" prop=\"endTime\">\r\n            <el-date-picker\r\n              v-model=\"orderForm.endTime\"\r\n              type=\"date\"\r\n              placeholder=\"选择结束日期\"\r\n              value-format=\"yyyy-MM-dd\"\r\n            ></el-date-picker>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-form-item>\r\n        <el-col :span=\"12\" :offset=\"18\" v-if=\"from === 'add'\">\r\n          <el-button type=\"primary\" @click=\"submitForm('orderForm')\" size=\"small\">立即创建</el-button>\r\n          <el-button @click=\"resetForm('orderForm')\" size=\"small\">取消</el-button>\r\n        </el-col>\r\n        <el-col :span=\"12\" :offset=\"18\" v-if=\"from === 'edit'\">\r\n          <el-button type=\"primary\" @click=\"submitForm('orderForm')\" size=\"small\">修改</el-button>\r\n          <el-button @click=\"handleCancelChange('orderForm')\" size=\"small\">取消</el-button>\r\n        </el-col>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"OrderForm\",\r\n  props: {\r\n    orderForm: {\r\n      type: Object,\r\n      value: {}\r\n    },\r\n    from: {\r\n      type: String,\r\n      value: \"add\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      rules: {\r\n        number: [\r\n          { required: true, message: \"请输入保单编号\", trigger: \"blur\" }\r\n        ],\r\n        type: [{ required: true, message: \"险种不能为空\", trigger: \"blur\" }],\r\n        insure: [\r\n          { required: true, message: \"投保人姓名不能为空\", trigger: \"blur\" }\r\n        ],\r\n        name: [\r\n          { required: true, message: \"被保人姓名不能为空\", trigger: \"blur\" }\r\n        ],\r\n        startTime: [\r\n          { required: true, message: \"起保日期不能为空\", trigger: \"blur\" }\r\n        ],\r\n        endTime: [\r\n          { required: true, message: \"截止日期不能为空\", trigger: \"blur\" }\r\n        ],\r\n        identify: [\r\n          { required: true, message: \"客户身份证号不能为空\", trigger: \"blur\" }\r\n        ],\r\n        pin: [{ required: true, message: \"序列号pin不能为空\", trigger: \"blur\" }]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          const order = JSON.parse(JSON.stringify(this.orderForm));\r\n          //console.log(order);\r\n          this.$emit(\"on-success\", order);\r\n        } else {\r\n          this.$message({\r\n            message: \"请将信息填写完整!\",\r\n            type: \"warning\"\r\n          });\r\n          console.log(\"error submit!!\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n      this.$emit(\"on-cancel\");\r\n    },\r\n    handleCancelChange(formName) {\r\n      this.$refs[formName].resetFields();\r\n      this.$emit(\"on-cancel\");\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components/Form"}]}