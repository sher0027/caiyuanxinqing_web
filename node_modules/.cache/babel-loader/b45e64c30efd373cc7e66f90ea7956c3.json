AAA;AACA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA;AACA,OAJA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OANA;AAOA;AAnGA,GA3BA;AAiIA,EAAA,OAjIA,qBAiIA;AACA,SAAA,gBAAA,CAAA,CAAA;AACA;AAnIA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row :gutter=\"20\" style=\"margin: 10px\">\r\n      <el-col :span=\"7\">\r\n        <div style=\"display: flex; flex-direction: row;\">\r\n          <el-input v-model=\"searchKey\" placeholder=\"请输入内容\" size=\"small\"></el-input>\r\n          <el-button\r\n            type=\"info\"\r\n            icon=\"el-icon-search\"\r\n            size=\"small\"\r\n            style=\"margin-left: 10px\"\r\n            @click=\"handleSearch\"\r\n          >搜索</el-button>\r\n        </div>\r\n      </el-col>\r\n    </el-row>\r\n    <el-row style=\"padding:10px\">\r\n      <el-table :data=\"accountList\" style=\"width: 100%\">\r\n        <el-table-column prop=\"bid\" label=\"ID\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"avatar\" label=\"头像\" width=\"90\">\r\n          <template slot-scope=\"scope\">\r\n            <el-avatar shape=\"square\" size=\"large\" :src=\"scope.row.avatar\"></el-avatar>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"name\" label=\"姓名\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"手机号\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"email\" label=\"邮箱\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"createdDatetimeString\" label=\"注册时间\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"lastLoginDatetimeString\" label=\"最后登录时间\" width=\"120\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"绑定信息\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button \r\n            @click.native.prevent=\"ViewAppoint(scope.row)\" \r\n            size=\"small\"\r\n            >知心预约</el-button>\r\n            <el-button\r\n            @click.native.prevent=\"ViewService(scope.row)\" \r\n            size=\"small\"\r\n            >尽心服务</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <el-dialog title=\"知心预约\" :visible.sync=\"dialogViewAppointVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"personalList\" style=\"width: 100%\">\r\n        <el-table-column prop=\"bookerId\" label=\"ID\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerName\" label=\"姓名\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerPhone\" label=\"手机号\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerRoleString\" label=\"身份\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerGrade\" label=\"年级\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerGender\" label=\"性别\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solutionString\" label=\"形式\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"statusString\" label=\"状态\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solverName\" label=\"对接老师\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solvedDatetimeString\" label=\"时间\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n          <template slot-scope=\"scope\">\r\n            <el-row :gutter=\"5\">\r\n              <el-col :span=\"10\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleGetAppointDetail(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >查看详情</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.statusString=='待指派'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleAppoint(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >指派</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.statusString=='待指派'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleClaim(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >认领</el-button>\r\n              </el-col>\r\n              <el-col :span=\"14\" v-show=\"scope.row.statusString=='待确认'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleAllocateTime(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >分配时间</el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>  \r\n    <el-dialog title=\"尽心服务\" :visible.sync=\"dialogViewServiceVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"serviceList\" style=\"width: 100%\">\r\n        <el-table-column type=\"index\" width=\"50\" ></el-table-column>\r\n        <el-table-column prop=\"leaverId\" label=\"ID\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"title\" label=\"标题\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"leaverName\" label=\"姓名\"></el-table-column>\r\n        <el-table-column prop=\"leaverPhone\" label=\"手机号\"></el-table-column>\r\n        <el-table-column prop=\"leaverEmail\" label=\"邮箱地址\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"leftDatetimeString\" label=\"留言时间\"></el-table-column>\r\n        <el-table-column prop=\"content\" label=\"内容\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n          <template slot-scope=\"scope\">\r\n            <el-row :gutter=\"5\">\r\n              <el-col :span=\"10\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleGetServiceDetail(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >查看详情</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.status=='1'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleConfirm(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >确认已回复</el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog> \r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogAppointDetailVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"appointDetail\" style=\"width: 100%\">\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"situationDesc\" label=\"主题描述\" width=\"360\"></el-table-column>\r\n        <el-table-column prop=\"reservedDatetimes\" label=\"有空时段\" width=\"120\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog title=\"指派\" :visible.sync=\"dialogAppointVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-select v-model=\"adminsList.name\" clearable placeholder=\"请选择老师\" style=\"width:70%\">\r\n        <el-option\r\n          v-for=\"item in adminsList\"\r\n          :key=\"item.aid\"\r\n          :label=\"item.name\"\r\n          :value=\"item.name\">\r\n        </el-option>\r\n      </el-select>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogAppointVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogAppointVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"认领\" :visible.sync=\"dialogClaimVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <span>认领该生？</span>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogClaimVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogClaimVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"分配时间\" :visible.sync=\"dialogAllocateTimeVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <div style=\"margin-bottom: 10px; display: flex; flex-direction: column\">\r\n        <span style=\"padding: 10px 0px; font-size: 15px\">时间描述</span>\r\n        <span style=\"padding: 20px 0px; color: #A9A9A90\"></span>\r\n        <span style=\"padding: 10px 0px 40px; font-size: 15px\">分配时间</span>\r\n        <el-row>\r\n          <el-col :span=\"8\">\r\n            <el-date-picker\r\n              v-model=\"date\"\r\n              type=\"date\"\r\n              placeholder=\"请选择日期\"\r\n              :picker-options=\"pickerOptions\">\r\n            </el-date-picker>\r\n          </el-col>  \r\n          <el-col :span=\"16\">\r\n            <el-time-picker\r\n              v-model=\"time\"\r\n              :picker-options=\"{\r\n                selectableRange: '18:30:00 - 20:30:00'\r\n              }\"\r\n              placeholder=\"请选择开始时间\">\r\n            </el-time-picker>\r\n          </el-col>  \r\n          <el-col :span=\"12\" :offset=\"20\">\r\n            <el-button @click=\"dialogAllocateTimeVisible = false\">确 定</el-button>\r\n          </el-col>  \r\n        </el-row>  \r\n      </div>\r\n    </el-dialog> \r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogServiceDetailVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"serviceDetail\" style=\"width: 100%\">\r\n        <el-table-column prop=\"title\" label=\"标题\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"content\" label=\"内容\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog title=\"确认已回复\" :visible.sync=\"dialogConfirmVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <span>确认已回复？</span>\r\n      <el-input\r\n        style=\"padding: 20px 0px\"\r\n        type=\"textarea\"\r\n        autosize\r\n        placeholder=\"请输入内容\"\r\n        v-model=\"reply\">\r\n      </el-input>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogConfirmVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogConfirmVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog> \r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>  \r\n\r\n<script>\r\nimport { getAccountList, searchAccount, searchAppoint, getAppointDetail } from \"@/api/account\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      accountList: [],\r\n      appointList: [],\r\n      serviceList: [],\r\n      appointDetail: [],\r\n      serviceDetail: [],\r\n      adminsList: [],\r\n      totalCount: 0,\r\n      pageIndex: 0,\r\n      searchKey: '',\r\n      reply: '',\r\n      //知心预约&尽心服务弹窗\r\n      dialogViewAppointVisible: false,\r\n      dialogViewServiceVisible: false,\r\n      //知心预约--弹窗\r\n      dialogAppointVisible: false,\r\n      dialogClaimVisible: false,\r\n      dialogAllocateTimeVisible: false,\r\n      dialogAppointDetailVisible: false,\r\n      //尽心服务--弹窗\r\n      dialogServiceDetailVisible: false,\r\n      dialogConfirmVisible: false\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchAccountList(pageIndex) {\r\n      getAccountList(pageIndex)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.accountList = res.data.pageList;\r\n          this.totalCount = res.data.pageList.length;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    ViewAppoint(){\r\n      this.dialogViewAppointVisible = true;\r\n    },\r\n\r\n    ViewService(){\r\n      this.dialogViewServiceVisible = true;\r\n    },\r\n\r\n    handleGetAppointDetail(row) {\r\n      const fid = row.fid;\r\n      this.dialogAppointDetailVisible = true;\r\n      getPersonalInsure(fid)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.detailList = res.data;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    handleGetServiceDetail(row) {\r\n      const fid = row.fid;\r\n      this.dialogServiceDetailVisible = true;\r\n      getPersonalInsure(fid)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.detailList = res.data;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    handleAppoint(row){\r\n      const fid = row.fid;\r\n      this.dialogAppointVisible = true;\r\n    },\r\n\r\n    handleClaim(row){\r\n      const fid = row.fid;\r\n      this.dialogClaimVisible = true;\r\n    },\r\n\r\n    handleAllocateTime(row){\r\n      const fid = row.fid;\r\n      this.dialogAllocateTimeVisible = true;\r\n    },\r\n\r\n    handleConfirm(row){\r\n      const fid = row.fid;\r\n      this.dialogConfirmVisible = true;\r\n    },\r\n\r\n\r\n    handleGetaccount(row) {\r\n      const fid = row.fid;\r\n      getAccountInsure(fid)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.accountList = res.data;\r\n          this.dialogTableVisible = true;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    //分页\r\n    handlePageChange(page){\r\n      this.pageIndex = page-1;\r\n      console.log(this.pageIndex)\r\n      this.fetchAccountList(this.pageIndex);\r\n    },\r\n\r\n    //搜索\r\n    handleSearch(){\r\n      console.log(this.searchKey);\r\n      const key = this.searchKey;\r\n      searchAccount(1, key).then(res=>{\r\n        // console.log(res)\r\n        this.logsList = res.data.pageList;\r\n        this.totalCount = res.data.pageList.length;\r\n      }).catch( err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.fetchAccountList(1);\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/account"}]}