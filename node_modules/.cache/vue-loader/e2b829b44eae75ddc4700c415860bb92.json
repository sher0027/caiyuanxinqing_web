\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogDetailVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"appointDetail\" style=\"width: 100%\">\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"situationDesc\" label=\"主题描述\" width=\"360\"></el-table-column>\r\n        <el-table-column prop=\"reservedDatetimes\" label=\"有空时段\" width=\"120\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog title=\"指派\" :visible.sync=\"dialogAppointVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-transfer\r\n        v-model=\"Data\"\r\n        :data=\"solverList\"\r\n        :titles=\"['可选', '已选']\"\r\n        :props=\"{key: 'id',label: 'name'}\"\r\n        @change=\"handleChange\"\r\n      ></el-transfer>\r\n    </el-dialog>\r\n    <el-dialog title=\"认领\" :visible.sync=\"dialogClaimVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <span>认领该生？</span>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogClaimVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogClaimVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"分配时间\" :visible.sync=\"dialogAllocateTimeVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <div style=\"margin-bottom: 10px; display: flex; flex-direction: column\">\r\n        <span style=\"padding: 10px 0px; font-size: 15px\">时间描述</span>\r\n        <span style=\"padding: 20px 0px; color: #A9A9A90\">{{ solvedDatetimeString }}</span>\r\n        <span style=\"padding: 10px 0px 40px; font-size: 15px\">分配时间</span>\r\n        <el-row>\r\n          <el-col :span=\"8\">\r\n            <el-date-picker\r\n              v-model=\"date\"\r\n              type=