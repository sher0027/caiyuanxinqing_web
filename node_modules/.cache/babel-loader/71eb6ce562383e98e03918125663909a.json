ndex,name) {\r\n  return request ({\r\n    url: '/admin/messagesOfUser?pageSize=100',\r\n    method: 'get',\r\n    params: {pageIndex,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 留言详情\r\n */\r\n\r\nexport function getServiceDetail(pageIndex,name) {\r\n  return request ({\r\n    url: '/admin/messageDetail',\r\n    method: 'get',\r\n    params: {pageIndex,name}\r\n  })\r\n}\r\n\r\n/**\r\n *账号管理 - 回复留言\r\n */\r\n\r\nexport function Reply(pageIndex,name) {\r\n  return request ({\r\n    url: '/admin/replyMessage',\r\n    method: 'put',\r\n    params: {pageIndex,name}\r\n  })\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"]}]}