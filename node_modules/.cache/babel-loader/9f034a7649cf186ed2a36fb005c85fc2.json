all\"\r\n                  :disabled=\"false\"\r\n                >查看详情</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.status=='1'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleConfirm(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >确认已回复</el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogServiceDetailVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"serviceDetail\" style=\"width: 100%\">\r\n        <el-table-column prop=\"title\" label=\"标题\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"content\" label=\"内容\"></el-table-column>\r\n      </el-table>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog title=\"确认已回复\" :visible.sync=\"dialogConfirmVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <span>确认已回复？</span>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogConfirmVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogConfirmVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog> \r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from \"../../utils\";\r\n// import { getserviceList, getserviceInsure } from \"@/api/service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      serviceList: [{\r\n        leaverId: '1',\r\n        title: '1',\r\n        leaverName: '1',\r\n        leaverPhone: '1',\r\n        leaverEmail: '1',\r\n        leftDatetimeString: '1',\r\n        content: '1',\r\n        status: '1'\r\n      },{\r\n        leaverId: '1',\r\n        title: '1',\r\n        leaverName: '1',\r\n        leaverPhone: '1',\r\n        leaverEmail: '1',\r\n        leftDatetimeString: '1',\r\n        content: '1',\r\n        status: '1'\r\n      }],\r\n      totalCount:0,\r\n      pageIndex:0,\r\n      dialogServiceDetailVisible: false,\r\n      dialogConfirmVisible: false\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchserviceList(pageIndex) {\r\n      getserviceList(pageIndex)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.clientList = res.data;\r\n          this.totalCount = res.data[0].count;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    \r\n    handleGetServiceDetail(row) {\r\n      const fid = row.fid;\r\n      this.dialogServiceDetailVisible = true;\r\n      getPersonalInsure(fid)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.detailList = res.data;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n\r\n    handleConfirm(row){\r\n      const fid = row.fid;\r\n      this.dialogConfirmVisible = true;\r\n    },\r\n\r\n\r\n    // 规范时间\r\n    formatTime(time) {\r\n      return parseTime(time);\r\n    },\r\n\r\n        // 分页\r\n    handlePageChange(page){\r\n      this.pageIndex = page-1;\r\n      console.log(this.pageIndex)\r\n      this.fetchserviceList(this.pageIndex);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchserviceList(0);\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/service"}]}