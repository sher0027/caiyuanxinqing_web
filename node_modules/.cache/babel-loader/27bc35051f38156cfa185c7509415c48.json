        <div style=\"padding: 14px;\">\r\n          <span>待确认预约</span>\r\n          <div class=\"card-body\" style=\"padding-left: 100px;padding-top: 10px;font-size: 40px;\">\r\n            <span>{{ waitingConfirm }}</span>\r\n          </div>\r\n        </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"4\" :offset=\"3\" style=\"padding-top: 30px\">\r\n        <el-card :body-style=\"{ padding: '0px'}\" shadow=\"always\" style=\"width: 240px;height: 120px;\">\r\n        <div style=\"padding: 14px;\">\r\n          <span>待开展预约</span>\r\n          <div class=\"card-body\" style=\"padding-left: 100px;padding-top: 10px;font-size: 40px;\">\r\n            <span>{{ waitingStart }}</span>\r\n          </div>\r\n        </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"4\" :offset=\"3\" style=\"padding-top: 30px\">\r\n        <el-card :body-style=\"{ padding: '0px'}\" shadow=\"always\" style=\"width: 240px;height: 120px;\">\r\n          <div style=\"padding: 14px;\">\r\n            <span>已完成预约</span>\r\n            <div class=\"card-body\" style=\"padding-left: 100px;padding-top: 10px;font-size: 40px;\">\r\n              <span>{{ finished }}</span>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <el-row style=\"padding: 30px 10px\">\r\n      <el-table :data=\"personalList\" style=\"width: 100%\">\r\n        <el-table-column prop=\"bookerId\" label=\"ID\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerName\" label=\"姓名\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerPhone\" label=\"手机号\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerRoleString\" label=\"身份\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerGrade\" label=\"年级\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"bookerGender\" label=\"性别\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solutionString\" label=\"形式\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"statusString\" label=\"状态\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solverName\" label=\"对接老师\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"solvedDatetimeString\" label=\"时间\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n          <template slot-scope=\"scope\">\r\n            <el-row :gutter=\"5\">\r\n              <el-col :span=\"10\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleGetApponitDetail(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >查看详情</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.statusString=='待指派'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleAppoint(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >指派</el-button>\r\n              </el-col>\r\n              <el-col :span=\"7\" v-show=\"scope.row.statusString=='待指派'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleClaim(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >认领</el-button>\r\n              </el-col>\r\n              <el-col :span=\"14\" v-show=\"scope.row.statusString=='待确认'\">\r\n                <el-button\r\n                  @click.native.prevent=\"handleAllocateTime(scope.row)\"\r\n                  size=\"small\"\r\n                  :disabled=\"false\"\r\n                >分配时间</el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogDetailVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"appointDetail\" style=\"width: 100%\">\r\n        <el-table-column prop=\"topic\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"situationDesc\" label=\"主题描述\" width=\"360\"></el-table-column>\r\n        <el-table-column prop=\"reservedDatetimes\" label=\"有空时段\" width=\"120\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog title=\"指派\" :visible.sync=\"dialogAppointVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-transfer\r\n        v-model=\"Data\"\r\n        :data=\"solverList\"\r\n        :titles=\"['可选', '已选']\"\r\n        :props=\"{key: 'id',label: 'name'}\"\r\n        @change=\"handleChange\"\r\n      ></el-transfer>\r\n    </el-dialog>\r\n    <el-dialog title=\"认领\" :visible.sync=\"dialogClaimVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <span>认领该生？</span>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogClaimVisible = false\">确 定</el-button>\r\n        <el-button @click=\"dialogClaimVisible = false\">取 消</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"分配时间\" :visible.sync=\"dialogAllocateTimeVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <div style=\"margin-bottom: 10px; display: flex; flex-direction: column\">\r\n        <span style=\"padding: 10px 0px; font-size: 15px\">时间描述</span>\r\n        <span style=\"padding: 20px 0px; color: #A9A9A90\">{{ solvedDatetimeString }}</span>\r\n        <span style=\"padding: 10px 0px 40px; font-size: 15px\">分配时间</span>\r\n        <el-row>\r\n          <el-col :span=\"8\">\r\n            <el-date-picker\r\n              v-model=\"date\"\r\n              type=\"date\"\r\n              placeholder=\"请选择日期\"\r\n              :picker-options=\"pickerOptions\">\r\n            </el-date-picker>\r\n          </el-col>  \r\n          <el-col :span=\"16\">\r\n            <el-time-picker\r\n              v-model=\"time\"\r\n              :picker-options=\"{\r\n                selectableRange: '18:30:00 - 20:30:00'\r\n              }\"\r\n              placeholder=\"请选择开始时间\">\r\n            </el-time-picker>\r\n          </el-col>  \r\n          <el-col :span=\"12\" :offset=\"20\">\r\n            <el-button @click=\"dialogAllocateTimeVisible = false\">确 定</el-button>\r\n          </el-col>  \r\n        </el-row>  \r\n      </div>\r\n    </el-dialog>\r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from \"../../utils\";\r\nimport { getPersonalList } from \"@/api/personal\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      waitingConfirm: 0,\r\n      waitingStart: 0,\r\n      finished: 0,\r\n      personalList: [],\r\n      // solverList: [{id:1,name:'测试1'},{id:2,name:'测试2'},{id:3,name:'测试3'}],\r\n      // data: [1,3],\r\n      pickerOptions: {\r\n        disabledDate(time) {\r\n          return time.getTime() > Date.now();\r\n        },\r\n        shortcuts: [{\r\n          text: '今天',\r\n          onClick(picker) {\r\n            picker.$emit('pick', new Date());\r\n          }\r\n        }, {\r\n          text: '昨天',\r\n          onClick(picker) {\r\n            const date = new Date();\r\n            date.setTime(date.getTime() - 3600 * 1000 * 24);\r\n            picker.$emit('pick', date);\r\n          }\r\n        }, {\r\n          text: '一周前',\r\n          onClick(picker) {\r\n            const date = new Date();\r\n            date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\r\n            picker.$emit('pick', date);\r\n          }\r\n        }]\r\n      },\r\n      totalCount: 0,\r\n      pageIndex: 0,\r\n      dialogAppointVisible: false,\r\n      dialogClaimVisible: false,\r\n      dialogAllocateTimeVisible: false,\r\n      dialogDetailVisible: false,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchPersonalList(pageIndex) {\r\n      getPersonalList(pageIndex)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.waitingConfirm = res.data.waitingConfirm,\r\n          this.waitingStart = res.data.waitingStart,\r\n          this.finished = res.data.finished,\r\n          this.personalList = res.data.myReservations.pageList;\r\n          this.totalCount = res.data.myReservations.pageList.length;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    handleGetApponitDetail(row) {\r\n      console.log(row)\r\n      this.dialogDetailVisible = true;\r\n    },\r\n\r\n    handleAppoint(row){\r\n      console.log(row)\r\n      this.dialogAppointVisible = true;\r\n    },\r\n\r\n    handleClaim(row){\r\n      console.log(row)\r\n      this.dialogClaimVisible = true;\r\n    },\r\n\r\n    handleAllocateTime(row){\r\n      console.log(row)\r\n      this.dialogAllocateTimeVisible = true;\r\n    },\r\n\r\n    handleChange(value, direction, movedKeys) {\r\n      console.log(value, direction, movedKeys);\r\n        //可以通过direction回调right/left 来进行操作，right：把数字移到右边，left把数据移到左边\r\n        if(direction === \"right\") {\r\n          \r\n        }\r\n        if(direction === \"left\") {\r\n          \r\n        }\r\n    },\r\n\r\n    formatTime(time) {\r\n      return parseTime(time);\r\n    },\r\n\r\n    // 分页\r\n    handlePageChange(page){\r\n      this.pageIndex = page-1;\r\n      console.log(this.pageIndex)\r\n      this.fetchPersonalList(this.pageIndex);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchPersonalList(0);\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/personal"}]}