\"width: 240px;height: 120px;\">\r\n          <div style=\"padding: 14px;\">\r\n            <span>待确认预约</span>\r\n            <div class=\"card-body\" style=\"padding-left: 100px;padding-top: 10px;font-size: 40px;\">\r\n              <span>{{ Finished }}</span>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <el-row style=\"padding: 30px 10px\">\r\n      <el-table :data=\"PersonalList\" style=\"width: 100%\">\r\n        <el-table-column type=\"index\" width=\"50\"></el-table-column>\r\n        <el-table-column prop=\"ID\" label=\"ID\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"姓名\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"手机号\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"identity\" label=\"身份\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"grade\" label=\"年级\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"gender\" label=\"性别\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"form\" label=\"形式\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"theme\" label=\"咨询主题\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"situation\" label=\"状态\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"teacher\" label=\"对接老师\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"时间\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" width=\"240\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              @click.native.prevent=\"handleGetDetail(scope.row)\"\r\n              type=\"text\"\r\n              size=\"small\"\r\n              :disabled=\"isDetailDisabled(scope.$index)\"\r\n            >查看详情</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <el-dialog title=\"详情\" :visible.sync=\"dialogTableVisible\" width=\"70%\" :close-on-click-modal=\"false\">\r\n      <el-table :data=\"detailList\">\r\n        <el-table-column prop=\"gid\" label=\"保单编号\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"type\" label=\"险种\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"insure\" label=\"投保人姓名\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"被保人姓名\" width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"startTime\" label=\"起保日期\" width=\"150\"></el-table-column>\r\n        <el-table-column prop=\"endTime\" label=\"截止日期\" width=\"150\"></el-table-column>\r\n        <el-table-column prop=\"identify\" label=\"身份证号\" width=\"160\"></el-table-column>\r\n        <el-table-column prop=\"pin\" label=\"序列号PIN\" width=\"150\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from \"../../utils\";\r\n// import { getAccountList, getAccountInsure } from \"@/api/account\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      Unconfirmed: 0,\r\n      Unfinished: 0,\r\n      Finished: 0,\r\n      PesonalList: [],\r\n      Personal:{\r\n        ID: '',\r\n        name: '',\r\n        phone: '',\r\n        identity: '',\r\n        grade: '',\r\n        gender: '',\r\n        form: '',\r\n        theme: '',\r\n        situation: '',\r\n        teacher: '',\r\n        time: ''\r\n      },\r\n      totalCount:0,\r\n      pageIndex:0\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchAccountList(pageIndex) {\r\n      getAccountList(pageIndex)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.clientList = res.data;\r\n          this.totalCount = res.data[0].count;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    handleGetDetail(row) {\r\n      const fid = row.fid;\r\n      getAccountInsure(fid)\r\n        .then(res => {\r\n          //console.log(res);\r\n          this.detailList = res.data;\r\n          this.dialogTableVisible = true;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    filterFlag(value, row) {\r\n      return row.flag === value;\r\n    },\r\n    isDetailDisabled(index) {\r\n      if (this.clientList[index].flag) {\r\n        return false;\r\n      } else {\r\n        return true;\r\n      }\r\n    },\r\n\r\n    // 规范时间\r\n    formatTime(time) {\r\n      return parseTime(time);\r\n    },\r\n\r\n        // 分页\r\n    handlePageChange(page){\r\n      this.pageIndex = page-1;\r\n      console.log(this.pageIndex)\r\n      this.fetchAccountList(this.pageIndex);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchAccountList(0);\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/personal"}]}