CA,WAAA,aAAA,CAAA,KAAA,SAAA;AACA;AA/BA,GAVA;AA4CA,EAAA,OA5CA,qBA4CA;AACA,SAAA,aAAA,CAAA,CAAA;AACA;AA9CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row :gutter=\"20\" style=\"margin: 10px\">\r\n      <el-col :span=\"7\">\r\n        <div style=\"display: flex; flex-direction: row;\">\r\n          <el-input v-model=\"searchKey\" placeholder=\"请输入内容\" size=\"small\" clearable></el-input>\r\n          <el-button\r\n            type=\"info\"\r\n            icon=\"el-icon-search\"\r\n            size=\"small\"\r\n            style=\"margin-left: 10px\"\r\n            @click=\"handleSearch\"\r\n          >搜索</el-button>\r\n        </div>\r\n      </el-col>\r\n    </el-row>\r\n    <el-row style=\"padding:10px\">\r\n      <el-table :data=\"logsList\" style=\"width: 100%\">\r\n        <el-table-column fixed prop=\"createdDatetimeString\" label=\"日期\" width=\"400\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"操作人\"></el-table-column>\r\n        <el-table-column prop=\"whatHappened\" label=\"操作内容\" width=\"100\"></el-table-column>\r\n      </el-table>\r\n    </el-row>\r\n    <div style=\"margin-bottom: 10px; display: flex; justify-content: center;\">\r\n      <el-pagination\r\n        background\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"totalCount\"\r\n        :page-size=\"20\"\r\n        @current-change=\"handlePageChange\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>  \r\n\r\n<script>\r\nimport { getLogsList, searchLogs } from\"@/api/logs\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      logsList: [],\r\n      searchKey: '',\r\n      totalCount: 0,\r\n      pageIndex: 0\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    fetchlogsList(pageIndex) {\r\n      getLogsList(pageIndex)\r\n        .then(res => {\r\n          // console.log(res)\r\n          this.logsList = res.data.pageList;\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    //搜索\r\n    handleSearch(){\r\n      console.log(this.searchKey);\r\n      const key = this.searchKey;\r\n      searchLogs(1, key).then(res=>{\r\n        // console.log(res)\r\n        this.logsList = res.data.pageList;\r\n        this.totalCount = res.data.pageList.length;\r\n      }).catch( err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n\r\n\r\n    //分页\r\n    handlePageChange(page) {\r\n      this.pageIndex = page - 1;\r\n      console.log(this.pageIndex);\r\n      this.fetchlogsList(this.pageIndex);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchlogsList(1);\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/logs"}]}